#include <iostream>
using namespace std;

// Recursive function to return nth Fibonacci number
int fib(int n) {
    if (n <= 1)
        return n;                 // Base cases: F(0) = 0, F(1) = 1
    return fib(n - 1) + fib(n - 2); // Recursive definition
}

int main() {
    int N;
    cout << "Enter number of terms: ";
    cin >> N;

    cout << "Fibonacci Series: ";
    for (int i = 0; i < N; i++) {
        cout << fib(i) << " ";
    }

    return 0;
}
